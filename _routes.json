{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/build/*",
    "/static/*", 
    "/_next/static/*",
    "/favicon.ico",
    "/robots.txt",
    "/*.ico",
    "/*.png",
    "/*.svg",
    "/*.jpg",
    "/*.jpeg",
    "/*.gif",
    "/*.webp",
    "/*.css",
    "/*.js",
    "/*.json",
    "/*.woff",
    "/*.woff2",
    "/*.ttf",
    "/*.otf"
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 